<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009"
			   xmlns:s="library://ns.adobe.com/flex/spark"
			   xmlns:mx="library://ns.adobe.com/flex/halo"
			   xmlns:swiz="http://swiz.swizframework.org"
			   minWidth="1024"
			   minHeight="768">

	<fx:Declarations>
		<swiz:SwizConfig strict="true"
						 mediateBubbledEvents="true"
						 viewPackages="com.soenkerohde.example.view"
						 eventPackages="com.soenkerohde.example.event"
						 beanLoaders="{[Beans]}" />
	</fx:Declarations>

	<view:AppView verticalCenter="0"
				  horizontalCenter="0"
				  xmlns:view="com.soenkerohde.example.view.*" />

</s:Application>
